{
    "manifest_version": 3,
    "version": "0.1.0",
    "name": "History quick view",
    "description": "History quick view",
    "author": "axarsar",
    "action": {
        "default_icon": {
            "16": "images/icon-toolbar-16.png",
            "32": "images/icon-toolbar-32.png"
        },
        "default_popup": "popup.html",
        "default_title": "History quick view"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "favicon",
        "storage",
        "tabs",
        "history"
    ]
}
